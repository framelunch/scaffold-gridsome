{
  "name": "scaffold-gridsome",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "gridsome build",
    "develop": "gridsome develop",
    "explore": "gridsome explore",
    "lint": "run-s lint:*",
    "lint:script": "eslint --ext .js 'src'",
    "lint:style": "stylelint './src/**/*.css'",
    "lint:vue": "run-s lint:vue:*",
    "lint:vue:script": "eslint --ext .vue 'src'",
    "lint:vue:style": "stylelint --config stylelint.config.vue.js './src/**/*.vue'",
    "fix": "run-s fix:*",
    "fix:script": "eslint --fix 'src/**/*.js'",
    "fix:style": "stylelint --fix 'src/**/*.css'",
    "fix:vue": "run-s fix:vue:*",
    "fix:vue:prettier": "prettier --write 'src/**/*.vue'",
    "fix:vue:eslint": "eslint --fix 'src/**/*.vue'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "gridsome": "^0.5.0"
  },
  "devDependencies": {
    "@mapbox/stylelint-processor-arbitrary-tags": "^0.2.0",
    "eslint": "^5.15.1",
    "eslint-config-framelunch": "framelunch/eslint-config-framelunch",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-gridsome": "^1.1.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.2.2",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "stylelint": "^9.10.1",
    "stylelint-config-framelunch": "framelunch/stylelint-config-framelunch",
    "stylelint-config-prettier": "^5.0.0",
    "vue-eslint-parser": "^6.0.3"
  }
}
